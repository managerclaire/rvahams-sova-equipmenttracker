<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<File name="FileLogger" fileName="EquipmentTracker.log" append="false">
			<PatternLayout pattern="%d %-5p %M - %m%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="all">
			<AppenderRef ref="FileLogger" />
		</Root>
	</Loggers>
</Configuration>